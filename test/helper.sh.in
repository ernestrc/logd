function assert_file_content {
	local __OUT=$(tr -d '\0' < $2)
	

	if [ "$__OUT" != "$1" ]; then
		echo "`date "+%Y-%m-%d %H:%M:%S"` expected '$1' but found '$__OUT'"
		if [ -f "$ERR" ]; then
			cat $ERR
		fi
		exit 1;
	fi
}


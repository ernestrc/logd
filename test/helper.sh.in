	cat >>$IN << EOF
2018-05-12 12:51:28 ERROR	[thread1]	clazz	a: A, 
2018-05-12 12:52:22 WARN	[thread2]	clazz	callType: b: B
EOF
	sync -f $IN
	sleep $TESTS_SLEEP
}

function assert_file_contains {
	COUNT=$(cat $2 | grep "$1" | wc -l)

	if [ "$COUNT" != "1" ]; then
		echo "expected to find '$1' in file but not found: $COUNT" 
		if [ -f "$ERR" ]; then
			cat $ERR
		fi
		cat $2
		exit 1;
	fi
}
